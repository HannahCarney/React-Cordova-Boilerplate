<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hannahcarney.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova-app</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <access launch-external="no" origin="*" />
    <access launch-external="no" origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="scripts/after_platform_add.js" type="before_run" />
    <hook src="scripts/build.js" type="before_prepare" />
    <hook src="scripts/Prepare.js" type="after_prepare" />
    <hook src="scripts/run.js" type="after_run" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
